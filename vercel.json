{
    "version": 2,
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ],
    "functions": {
      "index.js": {
        "maxDuration": 30,
        "memory": 1024
      }
    },
    "env": {
      "PUPPETEER_EXECUTABLE_PATH": "/usr/bin/google-chrome-stable"
    }
  }